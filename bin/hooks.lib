# shellcheck shell=sh

# Profile hooks library.
#

command -v _direnv_hook >/dev/null || { ! command -v hook >/dev/null || eval "$(direnv hook bash)"; }
type -p a

####################################### Executed: force & parse
#
if test "$#" -ne 0 && test "${PARSED-}"; then
  unset PARSED
  for arg do
    case "${arg}" in
      --force) FORCE=1 . "$0"
    esac
  done
elif test "$#" -ne 0; then
    PARSE="$0" parse "$@"
fi
