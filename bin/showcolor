#!/bin/bash

[ "${BASH_VERSION-}" ] || exit

#######################################
# Show colors
#######################################
main() {
  for i in ${COLORS}; do
    # shellcheck disable=SC3053
    printf "%b\n" "${!i}${i}${Reset}"
  done
  inf Info message
  warn Warning message
  err Error message
  false || die Die with error message
}

main
