#!/bin/sh

# rc_d_test script in sh for the repository README.adoc and no main() function
#
STRICT=1 . helpers.lib

if [ "${PARSED-}" ] && unset PARSED; then
  echo "DEBUG: ${DEBUG:-}"
  echo "DRYRUN: ${DRYRUN:-}"
  echo "QUIET: ${QUIET:-}"
  echo "VERBOSE: ${VERBOSE:-}"
  echo "WARNING: ${WARNING:-}"
  echo "args: ${*:-}"

  for arg do
    case "${arg}" in
      -h|--help) echo "Another help: ${arg}"; exit 1 ;;
      --other-option) echo "${arg}" ;;
      *) echo "${arg}" ;;
    esac
  done
else
  PARSE="${0}" parse "${@:-}"
fi

